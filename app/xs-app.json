{
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/favicon.ico$",
      "localDir": "./vue/"
    },
    {
      "source": "^/api(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "destination": "srv_api"
    },
    {
      "source": "^(.*)$",
      "localDir": "./vue/"
    }
  ]
}